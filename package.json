{
  "name": "starter-template",
  "version": "1.0.0",
  "description": "Node.js Appwrite 项目模板",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "start": "node src/main.js",
    "dev": "nodemon src/main.js",
    "format": "prettier --write .",
    "setup": "npm install && npm run dev",
    "package:dev": "./scripts/package.sh dev",
    "package:prod": "./scripts/package.sh prod",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "node-appwrite": "^12.0.1",
    "dotenv": "^16.3.1",
    "@aws-sdk/client-s3": "^3.485.0",
    "@aws-sdk/lib-storage": "^3.485.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "jest": "^29.7.0",
    "node-fetch": "^3.3.2"
  },
  "jest": {
    "transform": {},
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  }
} 